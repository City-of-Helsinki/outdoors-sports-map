{
  "name": "outdoors-sports-map",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "ts-node --transpile-only --project ./scripts/tsconfig.json ./scripts/generate-sitemap.ts && tsc && vite build",
    "preview": "vite preview",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!remark-breaks)/'",
    "test:coverage": "react-scripts test --transformIgnorePatterns 'node_modules/(?!remark-breaks)/' --ci --runInBand --collect-coverage",
    "eject": "react-scripts eject",
    "fetch-types": "flow-typed install",
    "flow": "flow",
    "lint": "eslint ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external"
            ],
            [
              "internal",
              "parent",
              "index",
              "sibling",
              "object"
            ]
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          }
        }
      ],
      "react/function-component-definition": [
        "error",
        {
          "namedComponents": "function-declaration"
        }
      ],
      "react/destructuring-assignment": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "url": "https://github.com/City-of-Helsinki/outdoors-sports-map.git",
    "type": "git"
  },
  "author": "Jukka-Pekka Salo <jpsalo@zoho.com>",
  "contributors": [],
  "dependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.25.9",
    "@react-leaflet/core": "2.1.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "1.9.14",
    "@types/lodash": "^4.17.5",
    "@types/node": "^18.11.19",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.12.0",
    "bootstrap": "5.2.3",
    "classnames": "^2.5.1",
    "d3": "^7.9.0",
    "date-fns": "^2.30.0",
    "hds-react": "^4.8.1",
    "history": "4.10.1",
    "husky": "^4.3.8",
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "leaflet": "1.9.4",
    "leaflet-geometryutil": "^0.10.3",
    "leaflet.heightgraph": "^1.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.6.2",
    "react": "18.3.1",
    "react-bootstrap": "2.10.10",
    "react-dom": "18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-hot-loader": "^4.13.1",
    "react-i18next": "^11.18.6",
    "react-leaflet": "4.2.1",
    "react-markdown": "^8.0.7",
    "react-redux": "^9.2.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "regenerator-runtime": "^0.14.1",
    "remark-breaks": "3.0.3",
    "sass": "^1.77.5",
    "shell-quote": "^1.8.1",
    "terser": "^5.31.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "web-vitals": "^1.1.2",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "cross-env": "^7.0.3",
    "prettier": "^3.3.2",
    "vite": "^7.3.1"
  },
  "overrides": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0"
  },
  "resolutions": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0"
  }
}
