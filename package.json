{
  "name": "outdoors-sports-map",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development react-scripts start",
    "build": "ts-node --transpile-only --project ./scripts/tsconfig.json ./scripts/generate-sitemap.ts && cross-env NODE_ENV=production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fetch-types": "flow-typed install",
    "flow": "flow",
    "lint": "eslint ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external"
            ],
            [
              "internal",
              "parent",
              "index",
              "sibling",
              "object"
            ]
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          }
        }
      ],
      "react/function-component-definition": [
        "error",
        {
          "namedComponents": "function-declaration"
        }
      ],
      "react/destructuring-assignment": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "url": "https://github.com/City-of-Helsinki/outdoors-sports-map.git",
    "type": "git"
  },
  "author": "Jukka-Pekka Salo <jpsalo@zoho.com>",
  "contributors": [],
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/jest": "^26.0.22",
    "@types/leaflet": "^1.7.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-leaflet": "^2.8.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-actions": "^2.6.1",
    "axios": "^0.21.2",
    "bootstrap": "4.6",
    "classnames": "^2.3.1",
    "date-fns": "^2.9.0",
    "hds-react": "^1.9.0",
    "history": "4.10.1",
    "husky": "^4.0.10",
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "leaflet": "1.7.0",
    "leaflet-geometryutil": "^0.8.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "node-sass": "^5.0.0",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.5.0",
    "react": "16.14.0",
    "react-bootstrap": "1.5.2",
    "react-dom": "16.14.0",
    "react-helmet-async": "^1.0.9",
    "react-hot-loader": "^4.12.19",
    "react-i18next": "^11.3.3",
    "react-leaflet": "2.8.0",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.1.3",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7",
    "remark-breaks": "2.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "prettier": "^2.2.1"
  }
}
