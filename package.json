{
  "name": "outdoors-sports-map",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build": "ts-node --transpile-only --project ./scripts/tsconfig.json ./scripts/generate-sitemap.ts && cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!remark-breaks)/'",
    "eject": "react-scripts eject",
    "fetch-types": "flow-typed install",
    "flow": "flow",
    "lint": "eslint ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external"
            ],
            [
              "internal",
              "parent",
              "index",
              "sibling",
              "object"
            ]
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          }
        }
      ],
      "react/function-component-definition": [
        "error",
        {
          "namedComponents": "function-declaration"
        }
      ],
      "react/destructuring-assignment": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "url": "https://github.com/City-of-Helsinki/outdoors-sports-map.git",
    "type": "git"
  },
  "author": "Jukka-Pekka Salo <jpsalo@zoho.com>",
  "contributors": [],
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/jest": "^26.0.22",
    "@types/leaflet": "^1.9.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.19",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-leaflet": "^2.8.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-actions": "^2.6.2",
    "axios": "^1.4.0",
    "bootstrap": "5.2.3",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "hds-react": "^2.9.0",
    "history": "4.10.1",
    "husky": "^4.0.10",
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "leaflet": "1.9.3",
    "leaflet-geometryutil": "^0.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.6.2",
    "react": "17.0.2",
    "react-bootstrap": "1.6.7",
    "react-dom": "17.0.2",
    "react-helmet-async": "^1.3.0",
    "react-hot-loader": "^4.13.1",
    "react-i18next": "^11.3.3",
    "react-leaflet": "2.8.0",
    "react-markdown": "^8.0.4",
    "react-redux": "^8.0.5",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-scripts": "4.0.3",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "regenerator-runtime": "^0.13.11",
    "remark-breaks": "3.0.2",
    "sass": "^1.62.1",
    "shell-quote": "^1.7.4",
    "terser": "^5.16.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "web-vitals": "^1.1.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "prettier": "^2.8.0"
  },
  "overrides": {
    "@types/react": "17.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.2"
  }
}
